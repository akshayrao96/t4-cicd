{
    "repo_name": "sample-repo",
    "repo_url": "https://github.com/sample-user/sample-repo",
    "branch": "main",
    "pipeline_name": "valid_pipeline",
    "pipeline_file_name": "valid_pipeline.yml",
    "pipelines": [
        {
          "pipeline_name": "valid_pipeline",
          "pipeline_file_name": "valid_pipeline.yml",
          "pipeline_config": {
            "global": {
              "pipeline_name": "valid_pipeline",
              "docker": {
                "registry": "dockerhub",
                "image": "ubuntu:latest"
              },
              "artifact_upload_path": "GitHub.com"
            },
            "stages": ["build", "test"],
            "jobs": {
              "checkout": {
                "stage": "build",
                "scripts": ["git clone"]
              },
              "compile": {
                "stage": "build",
                "allow_failure": true,
                "needs": ["checkout"],
                "scripts": ["poetry install"],
                "artifacts": {
                  "on_success_only": true,
                  "paths": ["build/lib"]
                }
              },
              "pytest": {
                "stage": "test",
                "scripts": ["poetry run pytest"]
              }
            }
          },
          "job_run_history": [],
          "active": true,
          "running": false,
          "last_commit_hash": "abc123def456"
        }
      ]
    }